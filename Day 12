#include <iostream>
#include <stack>
#include <string>
using namespace std;

bool isValid(string s) {
    stack<char> st;

    for (char c : s) {
        // Push opening brackets
        if (c == '(' || c == '{' || c == '[') {
            st.push(c);
        } 
        // Check closing brackets
        else {
            if (st.empty()) return false; // No matching opening bracket

            char top = st.top();
            st.pop();

            if ((c == ')' && top != '(') ||
                (c == '}' && top != '{') ||
                (c == ']' && top != '[')) {
                return false; // Mismatch
            }
        }
    }

    return st.empty(); // All brackets matched
}

int main() {
    string s;
    cout << "Enter string of brackets: ";
    cin >> s;

    if (isValid(s))
        cout << "true\n";
    else
        cout << "false\n";

    return 0;
}
